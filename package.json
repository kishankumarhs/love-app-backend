{
  "name": "@love-app/backend-monorepo",
  "version": "2.0.0",
  "description": "Love App Backend Microservices",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:gateway": "nest build api-gateway",
    "build:auth": "nest build auth-service",
    "build:notification": "nest build notification-service",
    "build:sos": "nest build sos-service",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:gateway": "nest start api-gateway",
    "start:auth": "nest start auth-service",
    "start:notification": "nest start notification-service",
    "start:sos": "nest start sos-service",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "docker:build:all": "docker-compose -f docker-compose.microservices.yml build",
    "docker:up": "docker-compose -f docker-compose.microservices.yml up -d",
    "docker:down": "docker-compose -f docker-compose.microservices.yml down",
    "docker:logs": "docker-compose -f docker-compose.microservices.yml logs -f",
    "k8s:deploy": "kubectl apply -f k8s/",
    "k8s:delete": "kubectl delete -f k8s/"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/microservices": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^11.1.8",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.8",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.3.2",
    "moment-timezone": "^0.6.0",
    "nestjs-i18n": "^10.5.1",
    "nodemailer": "^7.0.10",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "stripe": "^19.3.0",
    "twilio": "^5.10.4",
    "typeorm": "^0.3.27"
