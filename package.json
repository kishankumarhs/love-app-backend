  "packageManager": "npm@10.0.0",
  "scripts": {
    "build": "nest build",
    "build:api": "nest build api",
    "build:all": "nest build api && nest build auth && nest build users && nest build providers && nest build donations && nest build volunteers && nest build slots && nest build notifications && nest build audit && nest build connectivity && nest build common",
    "format": "prettier --write \"src/**/*.ts\" \"apps/**/*.ts\" \"libs/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:api": "nest start api",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/api/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:all": "./scripts/test.sh",
    "deploy": "./scripts/deploy.sh",
    "docker:build": "docker build -t loveapp-backend .",
    "docker:dev": "docker-compose up --build",
    "docker:down": "docker-compose down",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f api",
    "db:setup": "docker-compose up postgres redis -d",
    "db:migrate": "docker-compose exec postgres psql -U postgres -d love_app_dev -f /docker-entrypoint-initdb.d/001-create-core-tables.sql && docker-compose exec postgres psql -U postgres -d love_app_dev -f /docker-entrypoint-initdb.d/002-create-referrals-vouchers.sql && docker-compose exec postgres psql -U postgres -d love_app_dev -f /docker-entrypoint-initdb.d/003-create-slots-notifications.sql",
    "db:seed": "docker-compose exec postgres psql -U postgres -d love_app_dev -c \"$(cat seeds/001-seed-data.sql)\"",
    "db:reset": "npm run db:setup && sleep 5 && npm run db:migrate && npm run db:seed",
    "migration:run": "npm run db:migrate",
    "postman:test": "newman run postman/Love-App-MVP.postman_collection.json -e postman/Love-App-Environment.postman_environment.json"
  },
