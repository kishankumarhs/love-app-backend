    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "csv-writer": "^1.6.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "moment-timezone": "^0.6.0",
    "nestjs-i18n": "^10.5.1",
