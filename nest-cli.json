{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api/src",
  "monorepo": true,
  "root": "apps/api",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "users": {
      "type": "library",
      "root": "libs/users",
      "entryFile": "index",
      "sourceRoot": "libs/users/src",
      "compilerOptions": {
        "tsConfigPath": "libs/users/tsconfig.lib.json"
      }
    },
    "providers": {
      "type": "library",
      "root": "libs/providers",
      "entryFile": "index",
      "sourceRoot": "libs/providers/src",
      "compilerOptions": {
        "tsConfigPath": "libs/providers/tsconfig.lib.json"
      }
    },
    "donations": {
      "type": "library",
      "root": "libs/donations",
      "entryFile": "index",
      "sourceRoot": "libs/donations/src",
      "compilerOptions": {
        "tsConfigPath": "libs/donations/tsconfig.lib.json"
      }
    },
    "volunteers": {
      "type": "library",
      "root": "libs/volunteers",
      "entryFile": "index",
      "sourceRoot": "libs/volunteers/src",
      "compilerOptions": {
        "tsConfigPath": "libs/volunteers/tsconfig.lib.json"
      }
    },
    "slots": {
      "type": "library",
      "root": "libs/slots",
      "entryFile": "index",
      "sourceRoot": "libs/slots/src",
      "compilerOptions": {
        "tsConfigPath": "libs/slots/tsconfig.lib.json"
      }
    },
    "notifications": {
      "type": "library",
      "root": "libs/notifications",
      "entryFile": "index",
      "sourceRoot": "libs/notifications/src",
      "compilerOptions": {
        "tsConfigPath": "libs/notifications/tsconfig.lib.json"
      }
    },
    "audit": {
      "type": "library",
      "root": "libs/audit",
      "entryFile": "index",
      "sourceRoot": "libs/audit/src",
      "compilerOptions": {
        "tsConfigPath": "libs/audit/tsconfig.lib.json"
      }
    },
    "connectivity": {
      "type": "library",
      "root": "libs/connectivity",
      "entryFile": "index",
      "sourceRoot": "libs/connectivity/src",
      "compilerOptions": {
        "tsConfigPath": "libs/connectivity/tsconfig.lib.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    }
  }
}